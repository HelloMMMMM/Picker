<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="WheelView">
        <attr name="wl_textColor" format="color" />
        <attr name="wl_lineColor" format="color" />
        <attr name="wl_textSize" format="dimension" />
        <attr name="wl_isCircle" format="boolean" />
        <attr name="wl_showSize" format="integer" />
        <attr name="wl_offsetX" format="dimension" />
        <attr name="wl_alignMode" format="enum">
            <enum name="center" value="1"/>
            <enum name="left" value="2"/>
            <enum name="right" value="3"/>
        </attr>
        <attr name="wl_velocityRate" format="float" />
    </declare-styleable>
</resources>